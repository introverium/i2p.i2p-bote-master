</tag>

<tag>
    <name>printException</name>
    <tag-class>i2p.bote.web.PrintExceptionTag</tag-class>
    <body-content>empty</body-content>
    <attribute>
        <name>exception</name>
        <rtexprvalue>true</rtexprvalue>
