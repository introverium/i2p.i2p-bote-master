    <attribute>
        <name>destination</name>
        <rtexprvalue>true</rtexprvalue>
        <required>true</required>
    </attribute>
    <attribute>
        <name>pictureFilename</name>
        <rtexprvalue>true</rtexprvalue>
        <required>false</required>
    </attribute>
    <attribute>
        <name>text</name>
        <rtexprvalue>true</rtexprvalue>
        <required>false</required>
    </attribute>
</tag>

<tag>
    <name>connect</name>
    <tag-class>i2p.bote.web.ConnectTag</tag-class>
    <body-content>empty</body-content>
</tag>

<tag>
